<!DOCTYPE html>
<html>
<head>
  <title>Corey Nagel - <%=  yield(:title) %></title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
    <%= render 'layouts/header' %>
    <div class="mainbody">
        <div id="main">
          <%= yield %>
        </div>
      <div id="side">
        <%= render 'layouts/sidebar' %>
      </div>
    </div>
    <%= render 'layouts/footer' %>
</body>
</html>
